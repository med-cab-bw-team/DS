   "source": [
    "# Imports for NLP model:\n",
    "\n",
    "import spacy\n",
    "from sklearn.feature_extraction.text import TfidfVectorizer"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 38,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Instantiating the large English library:\n",
    "\n",
    "nlp = spacy.load(\"en_core_web_lg\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 39,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Creating a function to tokenize/lemmatize:\n",
    "\n",
    "def lemmatize(text):\n",
    "    \"\"\"\n",
    "    This function will tokenize the strings, and then returns\n",
    "    a list of lemmas.\n",
    "    \"\"\"\n",
    "    lemmas = []\n",
    "    processed = nlp(text)\n",
    "    \n",
    "    for token in processed:\n",
    "        if ((token.is_stop == False) and (token.is_punct == False)) and (token.pos_!= 'PRON'):\n",
    "            lemmas.append(token.lemma_)\n",
    "    return ' '.join(lemmas)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 40,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Updating merged_strings with lemmas:\n",
    "\n",
    "merged_strings['lemmas'] = merged_strings['all_text'].apply(lemmatize)"
   ]
  },
